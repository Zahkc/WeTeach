#content-all table.listing { /* fix whitespace */
	width:100%;
	margin-top:10px;
}
	
#content-all table.listing td { 
	font-weight:bold;
}
#content-all table.listing tr.content td { 
	font-weight:normal !important;
}

#content-all table.listing td span.content {
	display: inline-block;
	padding-left: 25px;
	padding-bottom: 12px;
	font-weight: normal !important;
} 

@media screen and (max-width:px) {
	.reducible {
		display: none !important;
	}
}

@media screen and (min-width:840px) {
	.smallscreen {
		display: none !important;
	}	
}

@media screen and (max-width:768px) {
	div#web-list {
		display: inherit !important;
	}

	.smallscreen {
		display: block !important;
	}

	#web-grid, #folder-tree {
		display: none !important;
	}

	.reverse-element, .reverse-wedge {
		display: none;
	}

	body #page-title-navigation {
		font-size: 22px !important;
	}

	body #everything h4 {
		font-size: 16px !important;
	}

	#web-list > div > div > div {
		display: table-row;
	}
}

@media screen and (max-width:695px) {
	#web-list > div > div > div {
		display: table-row;
	}

	#navigation {
		display: none !important;
	}

	a {
		width: inherit !important;
		overflow: ellipsis !important;
	}

}

@media screen and (max-width:445px) {


	#content-all {
		overflow: ellipsis;
	}

	#page-wrapper {
		/*	margin: auto; */
		margin-left: 20px !important;
		margin-right: 20px !important;
		max-width: 1032px !important;
		min-width: 440px !important;
	}
}